[include]
    path = .gitconfig.local
[color]
    ui = auto
    diff = auto
    status = auto
    branch = auto
[alias]
    ci = commit
    st = status -sb
    gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
[core]
    editor = vim -c \"set fenc=utf-8\"
    quotepath = false
[http]
      sslVerify = false
